mylog=`tail -1 logs.txt`

okstat=`echo $mylog | grep Ok`
okstat1=`echo $?`
warnstat=`echo $mylog | grep Warning`
warnstat1=`echo $?`
critstat=`echo $mylog | grep Critical`
critstat1=`echo $?`

if [ $okstat1 == 0 ]
then
 echo "Ok"
 exit 0
elif [ $warnstat1 == 0 ]
then
 echo "Warning"
 exit 1
elif [ $critstat1 == 0 ]
then
 echo "Critical"
 exit 2
else
 echo "Unknown"
 exit 3
fi 

